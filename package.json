{
  "name": "liliumai",
  "version": "1.0.0",
  "description": "LiliumAI - The elegant AI agent framework for Node.js",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "download-models": "ts-node scripts/download-models.ts",
    "build:bin": "npm run build && pkg .",
    "check-permissions": "python3 scripts/automate_mac_permissions.py",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": "dist/index.js",
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": [
      "node_modules/**/*",
      "dist/**/*"
    ],
    "targets": [
      "node18-linux-x64",
      "node18-macos-x64",
      "node18-win-x64"
    ],
    "outputPath": "bin"
  },
  "keywords": [
    "ai-agent",
    "llm",
    "chatbot",
    "wechaty",
    "openai",
    "ollama",
    "whisper",
    "local-ai",
    "nodejs",
    "typescript",
    "automation"
  ],
  "author": "LiliumAI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beidald/liliumai.git"
